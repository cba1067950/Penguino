<h1>TEST NEW RECORDS</h1>

<% if flash[:errors] %>
  <% flash[:errors].each do |error| %>
    <li><%= error %></li>
  <% end %>
<% end %>

<%= form_for @record do |f| %>
  <%= f.label "Project Name" %>
    <%= f.text_field :project_name %><br>
  <%= f.label "Colony" %>
    <%= f.collection_select :colony_id, @colonies, :id, :name %><br>
  <%= f.label "Location" %>
    <%=  f.text_field :location%><br>
  <%= f.submit %>
<% end  %>
